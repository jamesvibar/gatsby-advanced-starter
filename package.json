{
  "name": "gatsby-starter-advanced",
  "description": "GatsbyJS starter that includes examples for advanced use cases.",
  "version": "1.1.0",
  "author": "Ruben Harutyunyan <vagr9k@gmail.com>",
  "dependencies": {
    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.10",
    "dotenv": "^7.0.0",
    "fs": "^0.0.2",
    "gatsby": "5.13.3",
    "gatsby-image": "3.11.0",
    "gatsby-plugin-canonical-urls": "^5.0.0",
    "gatsby-plugin-catch-links": "5.13.1",
    "gatsby-plugin-emotion": "^8.0.0",
    "gatsby-plugin-feed": "5.13.1",
    "gatsby-plugin-google-analytics": "5.13.1",
    "gatsby-plugin-lodash": "6.13.1",
    "gatsby-plugin-manifest": "5.13.1",
    "gatsby-plugin-netlify": "^2.0.13",
    "gatsby-plugin-netlify-cache": "^1.1.0",
    "gatsby-plugin-nprogress": "5.13.1",
    "gatsby-plugin-offline": "6.13.1",
    "gatsby-plugin-react-helmet": "6.13.1",
    "gatsby-plugin-sharp": "2.0.32",
    "gatsby-plugin-sitemap": "6.13.1",
    "gatsby-plugin-twitter": "5.13.1",
    "gatsby-plugin-typography": "^5.0.0",
    "gatsby-remark-autolink-headers": "6.13.1",
    "gatsby-remark-copy-linked-files": "6.13.1",
    "gatsby-remark-images": "7.13.1",
    "gatsby-remark-prismjs": "7.13.1",
    "gatsby-remark-responsive-iframe": "6.13.1",
    "gatsby-source-contentful": "^8.0.0",
    "gatsby-source-filesystem": "5.13.1",
    "gatsby-transformer-remark": "^2.3.8",
    "gatsby-transformer-sharp": "^5.0.0",
    "lodash": "^4.17.4",
    "moment": "^2.23.0",
    "prismjs": "^1.15.0",
    "react": "^16.7.0",
    "react-disqus-comments": "^1.4.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "react-share": "^2.4.0",
    "react-twitter-widgets": "^1.7.1",
    "url-join": "^4.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.7.0",
    "cli-glob": "^0.1.0",
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-emotion": "^10.0.7",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "gh-pages": "^2.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "develop": "gatsby develop -o",
    "dev": "npm run develop",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "build:gh": "npm run clean && npm run build:pp && gh-pages -d public",
    "clean": "rm -rf public && rm -rf .cache",
    "lint": "eslint . --ext .js,.jsx --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .js,.jsx --fix --ignore-path .gitignore",
    "format:js": "prettier \"**/*.{js,jsx}\" --write"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint",
      "git add"
    ],
    "*.+(json|yml|yaml|css|less|scss|md|graphql|mdx)": [
      "prettier --write",
      "git add"
    ]
  }
}
